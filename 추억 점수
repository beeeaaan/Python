def solution(name, yearning, photo):
    dic = {name[i]: yearning[i] for i in range(len(name))}
    answer = []
    
    for case in photo:
        score = 0
        for person in case:
            # 이름 있는 경우 그리움 점수 추가
            if person in dic:
                score += dic[person]
        answer.append(score)

    return answer
