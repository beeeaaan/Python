def solution(number):
    answer = 0
    
    for i in range(len(number)):
        for j in range(i + 1, len(number)):
            for k in range(j + 1, len(number)):
                # 모두 더해서 0일 경우 삼총사
                if (number[i] + number[j] + number[k] == 0):
                    answer += 1

    return answer
