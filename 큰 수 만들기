def solution(number, k):
    stack = []
    
    for num in number: 
        # 제거할 수 k가 남았고 스택에 값 있고 마지막 값이 num보다 작은 경우
        while k > 0 and stack and stack[-1] < num:
            stack.pop()
            k -= 1
        stack.append(num)
    
    # k 남아있는 경우
    if k != 0:
        stack = stack[:-k]
    
    # 문자열로 변환 후 반환
    return ''.join(stack)
