def solution(data, col, row_begin, row_end):
    # col번째 컬럼 오름차순
    # 같을 경우 기본 키(첫번째 컬럼) 내림차순 정렬
    data.sort(key=lambda row: (row[col - 1], -row[0]))
    answer = 0

    for row_index in range(row_begin - 1, row_end):
        mod_sum = 0
        row_number = row_index + 1  # 사용하는 행 번호

        # 값 % 행번호의 합 계산
        for value in data[row_index]:
            mod_sum += value % row_number
        # 누적 XOR
        answer ^= mod_sum

    return answer
