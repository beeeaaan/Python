def hanoi(start, end, middle, answer, n):
	# 시작점 개수가 1개일 경우 도착점으로 이동
    if n == 1:
        answer.append([start, end])
    else:
        # 시작점 n-1개 중간지점으로 이동
        hanoi(start, middle, end, answer, n-1)
        # 다 옮긴 경우 시작점 1개 도착점으로 이동
        answer.append([start, end])
        # 중간 지점 시작점으로 다시 시작
        hanoi(middle, end, start, answer, n-1)
        
def solution(n):
    answer = []
    hanoi(1, 3, 2, answer, n)
        
    return answer
